<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:textSize="11sp">


    <TextView
        android:id="@+id/textView6"
        android:layout_width="245dp"
        android:layout_height="49dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="84dp"
        android:fontFamily="sans-serif-black"
        android:text="Add your Ride"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="327dp"
        android:layout_height="444dp"
        android:layout_marginTop="188dp"
        android:background="@drawable/rectangle"
        android:elevation="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/autocomplete_fragment2"
        android:name="com.example.carpoolbuddy.controllers.adapters.ThemedAutocompleteSupportFragment"
        android:layout_width="317dp"
        android:layout_height="43dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="560dp"
        android:elevation="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="37dp"
        android:layout_height="40dp"
        android:layout_marginStart="316dp"
        android:layout_marginTop="356dp"
        android:elevation="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_people_24"
        app:tint="@color/quantum_purple" />

    <EditText
        android:id="@+id/price"
        android:layout_width="286dp"
        android:layout_height="61dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="276dp"
        android:backgroundTint="#FAF5F5"
        android:elevation="9dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:hint="Price per Person"
        android:inputType="numberDecimal"
        android:outlineProvider="none"
        android:textColor="@color/black"
        android:textColorHint="#BCBABA"
        app:boxCornerRadiusBottomEnd="5dp"
        app:boxCornerRadiusBottomStart="5dp"
        app:boxCornerRadiusTopEnd="5dp"
        app:boxCornerRadiusTopStart="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="166dp"
        android:layout_height="70dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="200dp"
        android:drawSelectorOnTop="true"
        android:dropDownVerticalOffset="-53dp"
        android:elevation="10dp"
        android:spinnerMode="dropdown"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pText"
        android:layout_width="176dp"
        android:layout_height="45dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="496dp"
        android:elevation="10dp"
        android:fontFamily="sans-serif-medium"
        android:text="Pick-up Location"
        android:textColor="#BCBABA"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="327dp"
        android:layout_height="1dp"
        android:layout_marginTop="340dp"
        android:elevation="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/divider_line_dim" />

    <EditText
        android:id="@+id/time"
        android:layout_width="286dp"
        android:layout_height="61dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="412dp"
        android:backgroundTint="#FAF5F5"
        android:elevation="9dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:hint="Departure Time"
        android:inputType="datetime"
        android:outlineProvider="none"
        android:textColor="@color/black"
        android:textColorHint="#BCBABA"
        android:textSize="18sp"
        app:boxCornerRadiusBottomEnd="5dp"
        app:boxCornerRadiusBottomStart="5dp"
        app:boxCornerRadiusTopEnd="5dp"
        app:boxCornerRadiusTopStart="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/type"
        android:layout_width="286dp"
        android:layout_height="61dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="224dp"
        android:backgroundTint="#FAF5F5"
        android:elevation="9dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:hint="Vehicle Type"
        android:inputType="text"
        android:outlineProvider="none"
        android:textColor="@color/black"
        android:textColorHint="#BCBABA"
        android:textSize="18sp"
        app:boxCornerRadiusBottomEnd="5dp"
        app:boxCornerRadiusBottomStart="5dp"
        app:boxCornerRadiusTopEnd="5dp"
        app:boxCornerRadiusTopStart="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="327dp"
        android:layout_height="1dp"
        android:layout_marginTop="408dp"
        android:elevation="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/divider_line_dim" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="327dp"
        android:layout_height="1dp"
        android:layout_marginTop="544dp"
        android:elevation="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/divider_line_dim" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="327dp"
        android:layout_height="1dp"
        android:layout_marginTop="272dp"
        android:elevation="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/divider_line_dim" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="327dp"
        android:layout_height="1dp"
        android:layout_marginTop="472dp"
        android:elevation="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/divider_line_dim" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/autocomplete_fragment"
        android:name="com.example.carpoolbuddy.controllers.adapters.ThemedAutocompleteSupportFragment"
        android:layout_width="317dp"
        android:layout_height="43dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="488dp"
        android:elevation="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dText"
        android:layout_width="176dp"
        android:layout_height="45dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="568dp"
        android:elevation="10dp"
        android:fontFamily="sans-serif-medium"
        android:text="Drop-off Location"
        android:textColor="#BCBABA"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/timepicker"
        android:layout_width="37dp"
        android:layout_height="40dp"
        android:layout_marginStart="316dp"
        android:layout_marginTop="420dp"
        android:elevation="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_calendar_month_24"
        app:tint="@color/quantum_purple" />

    <EditText
        android:id="@+id/capacity"
        android:layout_width="286dp"
        android:layout_height="61dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="344dp"
        android:backgroundTint="#FAF5F5"
        android:elevation="9dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:hint="Capacity"
        android:inputType="number"
        android:outlineProvider="none"
        android:textColor="@color/black"
        android:textColorHint="#BCBABA"
        app:boxCornerRadiusBottomEnd="5dp"
        app:boxCornerRadiusBottomStart="5dp"
        app:boxCornerRadiusTopEnd="5dp"
        app:boxCornerRadiusTopStart="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="341dp"
        android:layout_height="41dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="132dp"
        android:fontFamily="sans-serif-medium"
        android:text="Enter ride information"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="762dp"
        android:onClick="back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/back" />


</androidx.constraintlayout.widget.ConstraintLayout>